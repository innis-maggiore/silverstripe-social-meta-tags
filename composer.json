{
    "name": "innis-maggiore/silverstripe-social-meta-tags",
    "description": "Adds social media meta tags to Silverstripe sites.",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/innis-maggiore/silverstripe-social-meta-tags",
    "keywords": ["silverstripe", "social media", "meta tags"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Joe Chenevey",
            "email": "web-dev@innismaggiore.com"
        }
    ],
    "support": {
        "issues": "https://github.com/innis-maggiore/silverstripe-social-meta-tags/issues"
    },
    "require": {
        "php": ">=8.3",
        "silverstripe/cms": "^6.0"
    },
    "replace": {
        "creativecodelabs/silverstripe-social-meta-tags": "*"
    },
    "autoload": {
        "psr-4": {
            "InnisMaggiore\\SilverstripeSocialMetaTags\\": "src/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
